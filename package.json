{
	"name": "featherplot",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@modyfi/vite-plugin-yaml": "^1.0.4",
		"@smui-extra/autocomplete": "^7.0.0-beta.8",
		"@smui/button": "^7.0.0-beta.8",
		"@smui/card": "^7.0.0-beta.8",
		"@smui/checkbox": "^7.0.0-beta.8",
		"@smui/chips": "^7.0.0-beta.8",
		"@smui/form-field": "^7.0.0-beta.8",
		"@smui/icon-button": "^7.0.0-beta.8",
		"@smui/menu-surface": "^7.0.0-beta.8",
		"@smui/select": "^7.0.0-beta.8",
		"@smui/slider": "^7.0.0-beta.8",
		"@smui/switch": "^7.0.0-beta.8",
		"@smui/textfield": "^7.0.0-beta.8",
		"@smui/top-app-bar": "^7.0.0-beta.8",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^2.0.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"autoprefixer": "^10.4.14",
		"daisyui": "^3.1.0",
		"postcss": "^8.4.24",
		"publint": "^0.1.9",
		"smui-theme": "^7.0.0-beta.8",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"vitest": "^0.25.3"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"deepscatter": "^2.11.1",
		"esm-env": "^1.0.0",
		"js-yaml": "^4.1.0"
	}
}
